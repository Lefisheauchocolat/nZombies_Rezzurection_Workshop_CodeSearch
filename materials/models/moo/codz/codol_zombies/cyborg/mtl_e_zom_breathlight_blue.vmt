"VertexLitGeneric"
{
    $basetexture "$invis"

	"$model" 1
    $translucent 1

	"$emissiveBlendEnabled" "1"
	$EmissiveBlendTexture 		vgui/white
	"$emissiveBlendBaseTexture" "models/$white_specular"
	"$emissiveBlendFlowTexture" "dev/null"
	"$emissiveBlendTint" "[0 0.15 1]"
	"$emissiveBlendStrength" "0.5"

	"$color2" "[1 1 1]"

	Proxies
    {
        ZombieEyeGlow
        {
            srcVar1 $emissiveblendtint
            srcVar2 $emissiveblendstrength
        }
    }
}
