"VertexlitGeneric"
{

    $basetexture                "nz_moo/camo/stained_glass/rainbow_lyr1"
    $bumpmap                    "null-bumpmap"
    $basetexturetransform       "center .5 .5 scale 2 2 rotate 0 translate 0 0"
    $color2 "[1 1 1]"
    
    $selfillum                "1"
    $selfillummask            "vgui/white"
    $selfillumtint            "[0.5 0.5 0.5]"
    
    $detail                    "nz_moo/camo/stained_glass/rainbow_lyr2"
    $detailblendmode        "2"
    $detailscale            "1"
    $detailblendfactor        "1"
    
    $phong    "1"
    $phongboost    "0.1"
    $phongfresnelranges    "[.78 .9 1]"
    $phongexponent "1"
    
    $envmap "env_cubemap"
    $envmaptint    "[0.1 0.1 0.1]"
    $normalmapalphaenvmapmask    "1"
    $envmapfresnel    "1"
    
    $emissiveblendenabled        "1"
    $emissiveblendstrength        "0.15"
    $emissiveblendtexture        "vgui/white"
    $emissiveblendbasetexture    "vgui/white"
    $emissiveblendflowtexture    "models/vortigaunt/vortigaunt_flow"
    $emissiveblendtint            "[0 0 0]"
    $emissiveblendscrollvector    "[0.1 0.1]"
    
    Proxies
    {
        TextureScroll
        {
            "textureScrollVar" "$basetexturetransform"
            "textureScrollRate" "0.05"
            "textureScrollAngle" "45"
        }
    }
}